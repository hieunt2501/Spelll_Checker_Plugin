{
    "manifest_version": 2,
    "short_name": "Spell_check",
    "name": "Spell Checker",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://mail.google.com/*"
          ],
          "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "spongebob.png"
    }
}